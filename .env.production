# Creator Suite Production Environment
DEBUG=false
LOG_LEVEL=INFO
ENVIRONMENT=production

# API URLs
FRONTEND_URL=https://video.destinpq.com
API_URL=https://video-api.destinpq.com

# Database settings (you'll need to configure these)
DB_HOST=localhost
DB_NAME=creator_suite_db
DB_USER=creator_suite
DB_PASS=creator_suite_password

# JWT settings
SECRET_KEY=your_secret_key_here_replace_with_actual_secure_key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=43200

# Redis settings
REDIS_URL=redis://localhost:6379/0

# Azure Storage (using the one from config.py)
AZURE_STORAGE_CONNECTION_STRING=BlobEndpoint=https://realign.blob.core.windows.net/;QueueEndpoint=https://realign.queue.core.windows.net/;FileEndpoint=https://realign.file.core.windows.net/;TableEndpoint=https://realign.table.core.windows.net/;SharedAccessSignature=sv=2024-11-04&ss=bfqt&srt=sco&sp=rwdlacupiytfx&se=2026-08-09T20:00:29Z&st=2025-07-30T11:45:29Z&spr=https,http&sig=%2BwP1aalEDXbg2lQ7FPUA%2BSPaAeXAnHBL5bOEIzQkt2o%3D
AZURE_VIDEO_CONTAINER_NAME=ai-video

# API Keys (you'll need to add your actual keys)
REPLICATE_API_TOKEN=your_replicate_token_here
GEMINI_API_KEY=your_gemini_key_here
RUNWAY_API_KEY=your_runway_key_here
OPENAI_API_KEY=your_openai_key_here

# Bot tokens (add if needed)
DISCORD_BOT_TOKEN=your_discord_token_here
TELEGRAM_BOT_TOKEN=your_telegram_token_here
WHATSAPP_ACCESS_TOKEN=your_whatsapp_token_here
WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_id_here
WHATSAPP_VERIFY_TOKEN=your_whatsapp_verify_token_here
INSTAGRAM_ACCESS_TOKEN=your_instagram_token_here

# API Base URL for bots
API_BASE_URL=https://video-api.destinpq.com/api/v1

# Runway service configuration
RUNWAY_GEN3_SERVICE_ID=5

# Instagram API
INSTAGRAM_APP_ID=your_instagram_app_id_here
INSTAGRAM_APP_SECRET=your_instagram_app_secret_here

# Payment
RAZORPAY_KEY_ID=your_razorpay_key_id_here
RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here
